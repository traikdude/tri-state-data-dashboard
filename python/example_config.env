# Example Configuration File
# Copy this to .env and fill in your values
# DO NOT commit .env to version control!

# ===========================================
# REQUIRED CONFIGURATION
# ===========================================

# Google Apps Script Web App URL
# Get this from: Deploy > Manage deployments > Copy web app URL
# Should look like: https://script.google.com/macros/s/DEPLOYMENT_ID/exec
GAS_WEBAPP_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec

# ===========================================
# OPTIONAL CONFIGURATION
# ===========================================

# Request timeout in seconds (default: 30)
REQUEST_TIMEOUT=30

# Maximum retry attempts for failed requests (default: 3)
MAX_RETRIES=3

# Email for quota warning notifications
ALERT_EMAIL=your-email@example.com

# Enable debug logging (true/false)
DEBUG_MODE=false
